local ListGame = {
	["6216468795"] = "a30565cbe5aaa1e8766db3e2b2c4d5bd", -- Weak Legacy 2
}

function GetExecutor()
	if identifyexecutor then
		local ok, n = pcall(identifyexecutor)
		if ok and type(n) == "string" then return n end
	end
	if getexecutor then
		local ok, n = pcall(getexecutor)
		if ok and type(n) == "string" then return n end
	end
	return "Unknown"
end

local Exec = GetExecutor() or "Unknown"
local GameId = ListGame[tostring(game.GameId)]

if string.find(Exec, "Xeno") or string.find(Exec, "Solara") then
	game:GetService("Players").LocalPlayer:Kick("This executor is not supported for this script.")
end

if not GameId then
	game:GetService("Players").LocalPlayer:Kick("This script dosent support this game")
else
	loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/" .. GameId .. ".lua"))()
end
